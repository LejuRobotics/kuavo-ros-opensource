Q_base_stance
{
  scaling 1e+0

  ;; Normalized Centroidal Momentum: [linear, angular] ;;
  (0,0)   13.0     ; vcom_x
  (1,1)   13.0     ; vcom_y
  (2,2)   13.0     ; vcom_z
  (3,3)   50.0      ; L_x / robotMass
  (4,4)   50.0     ; L_y / robotMass
  (5,5)   13.0     ; L_z / robotMass

